{% assign alt_text = 'felicitails account center, emotional support animal certification, psychiatric service animal certification,  dog and cat training, pet insurance, dog walking, pet sitting, dog boarding, dog dna testing, curated supplies and products, felicitails and free my paws founder lindsay giguiere ' %}

{% assign img_account_center_services = 'https://cdn.shopify.com/s/files/1/0068/5454/1410/files/felicitails-account-center-services-ordered-founded-by-lindsay-giguiere.png?v=1685640384' %}
{% assign alt_account_center_services = 'felicitails-account-center-services-ordered-founded-by-lindsay-giguiere' %}

{% assign img_account_center_registration = 'https://cdn.shopify.com/s/files/1/0068/5454/1410/files/felicitails-founded-by-lindsay-giguiere-account-center-find-your_esa-psd-by-registration-number.png?v=1685640385' %}

<div class="account-main-body">
  <div class="account-main-items">
    <div class="account-main-digitalvault">
      <div class="account-main-item-title">
        <h1>Services</h1>
      </div>
      <div class="account-main-item-image">
        <img
          src="{{ img_account_center_services }}"
          alt="{{ alt_account_center_services }}"
        >
      </div>
    </div>
    <div class="filter_buttons">
      <div class="filter_button height-fill">
        <span>Emotional Support Animal</span>
      </div>
      <div class="filter_button height-fill">
        <span>Psychiatric Service Dog</span>
      </div>
      <div class="filter_button height-fill active">
        <span>Registration Lookup</span>
      </div>
    </div>
    <div class="service-body width-fill">
      <img
        src="{{ img_account_center_registration }}"
        alt="{{ alt_text }}"
      >
      <div class="width-fill">
        <h4 class="width-fill">Registration Lookup</h4>
        <div class="service-infos width-fill">
          <div class="width-fill">
            <label for="regist-id" class="width-fill">Registration ID (include all characters)</label>
            <input
              type="text"
              name="regist-id"
              id="regist-id"
              class="width-fill"
            >
          </div>
          <div class="width-fill">
            <label for="last-name" class="width-fill">Last name of Registered Owner</label>
            <input
              type="text"
              name="last-name"
              id="last-name"
              class="width-fill"
            >
          </div>
        </div>
        <div class="width-fill">
          <button>FIND</button>
        </div>
      </div>
    </div>
  </div>
</div>
